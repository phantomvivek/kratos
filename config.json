{
    "config": {
        "url": "ws://localhost:8080/",
        "timeout": 5
    },
    "hitrate": [{
        "end": 100,
        "duration": 10
    },
    {
        "end": 100,
        "duration": 10
    },
    {
        "end": 0,
        "duration": 10
    }],
    "tests": [{
        "type": "message",
        "send": "${0} and ${1}",
        "replace": true
    },
    {
        "type": "message",
        "send": "${1} next and ${2}",
        "replace": true
    },
    {
        "type": "message",
        "send": "${1} no replace ${2}"
    },
    {
        "type": "sleep",
        "duration": 2
    },
    {
        "type": "disconnect"
    }],
    "dataFile": "tests/datafile.csv",
    "reporter": {
        "type": "statsd",
        "host": "127.0.0.1",
        "port": 8125,
        "prefix": "testproject"
    }
}